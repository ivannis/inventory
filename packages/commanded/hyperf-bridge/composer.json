{
    "name": "commanded/hyperf-bridge",
    "type": "project",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.4",

        "commanded/framework": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.12"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Commanded\\HyperfBridge\\": "src"
        },
        "files": [
            "./src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Commanded\\HyperfBridge\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "hyperf": {
            "config": "Commanded\\HyperfBridge\\ConfigProvider"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse --memory-limit 4096M -l 5 -c phpstan.neon",
        "cs-fix": "php-cs-fixer fix $1"
    }
}
