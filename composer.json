{
    "name": "ivannis/inventory",
    "type": "project",
    "description": "",
    "license": "MIT",
    "repositories": [
        { "type": "path", "url": "packages/commanded/framework", "options": {"symlink": true} },
        { "type": "path", "url": "packages/commanded/hyperf-bridge", "options": {"symlink": true} }
    ],
    "require": {
        "php": ">=7.4",
        "ext-swoole": ">=4.5",
        "hyperf/command": "~2.0.0",
        "hyperf/config": "~2.0.0",
        "hyperf/framework": "~2.0.0",
        "hyperf/guzzle": "~2.0.0",
        "hyperf/http-server": "~2.0.0",
        "hyperf/logger": "~2.0.0",
        "hyperf/memory": "~2.0.0",
        "hyperf/process": "~2.0.0",
        "hyperf/constants": "~2.0.0",
        "cache/filesystem-adapter": "^1.0",
        "hyperf/paginator": "^2.0",
        "hyperf/view": "^2.0",
        "hyperf/task": "^2.0",
        "hyperf/db-connection": "^2.0",
        "hyperf/async-queue": "^2.0",
        "league/csv": "^9.6",
        "hyperf/validation": "^2.0",
        "hyperf/database": "^2.0",
        "commanded/framework": "dev-master",
        "commanded/hyperf-bridge": "dev-master"
    },
    "require-dev": {
        "swoole/ide-helper": "^4.5",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "hyperf/devtool": "~2.0.0",
        "hyperf/testing": "~2.0.0"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "autoload": {
        "psr-4": {
            "Stock\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "./test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": [],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "rm -rf runtime/container"
        ],
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src ./config",
        "start": "php ./bin/hyperf.php start",
        "serve": "cd resources && serve -s build"
    }
}
